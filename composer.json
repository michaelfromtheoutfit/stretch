{
    "name": "jayi/stretch",
    "version": "0.5.0",
    "description": "A Laravel package for building Elasticsearch queries with a fluent, intuitive API",
    "keywords": [
        "laravel",
        "elasticsearch",
        "query-builder",
        "search",
        "stretch"
    ],
    "homepage": "https://github.com/jayi/stretch",
    "license": "MIT",
    "authors": [
        {
            "name": "Jay Fletcher",
            "email": "jayjfletcher@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^12.0",
        "elasticsearch/elasticsearch": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.0"
    },
    "require-dev": {
        "brianium/paratest": "^7.0",
        "fakerphp/faker": "^1.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "JayI\\Stretch\\": "src/",
            "JayI\\Stretch\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JayI\\Stretch\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-stretch --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JayI\\Stretch\\StretchServiceProvider"
            ],
            "aliases": {
                "Stretch": "JayI\\Stretch\\Facades\\Stretch"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
